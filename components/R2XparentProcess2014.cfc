<cfcomponent name="R2XstudentProcess2014" hint="adds a level of abstraction for web,mobile to use the same code">	 <!---init--->
<cffunction name="init" access="public" output="false">
		<cfreturn this />
</cffunction>			<!--- *** 	RETURN: Boolean, based on whether or not item was added	*** --->    <cffunction name="setComplete" returntype="boolean">    	<cfargument name="banneridIn">        <cfargument name="itemCodeIn">        <cfargument name="peIn" required="no" default="">            <cftry>                <cfif this.isProcessItemComplete(arguments.banneridIn,arguments.itemCodeIn) or session.parentlogin is TRUE >        	<cfreturn false>        <cfelse>                    <cfquery datasource="#application.roadtoDB#">            insert into processParent(bannerid,itemCode,date_completed,parentEmail)            values(                <cfqueryparam value="#arguments.banneridIn#" cfsqltype="cf_sql_varchar">,                <cfqueryparam value="#arguments.itemCodeIn#" cfsqltype="cf_sql_varchar">,                <cfqueryparam value="#createODBCDateTime(now())#" cfsqltype="cf_sql_timestamp">,                <cfqueryparam value="#arguments.peIn#" cfsqltype="cf_sql_varchar">            )            </cfquery>                        <cfreturn true>    	</cfif>    	    	    		<cfcatch>    		    		 <cfmail from="sparkse1@xavier.edu" to="sparkse1@xavier.edu" subject="R2X setComplete fail" type="html">    		   <cfdump var="#cfcatch#">     		 </cfmail>    		 		<cfreturn false>    		</cfcatch>    	    	</cftry>    	    </cffunction>        <!--- *** 	RETURN: Void. Just add the item as incomplete.	*** --->    <cffunction name="setIncomplete" returntype="boolean">    	<cfargument name="banneridIn">        <cfargument name="itemCodeIn">        <cfargument name="peIn" required="no" default="">                <cfquery datasource="#application.roadtoDB#">        update processParent        set date_deleted = <cfqueryparam value="#createODBCDateTime(now())#" cfsqltype="cf_sql_timestamp">        where bannerID = <cfqueryparam value="#arguments.banneridIn#" cfsqltype="cf_sql_varchar">          and itemCode = <cfqueryparam value="#arguments.itemCodeIn#" cfsqltype="cf_sql_varchar">          and parentEmail = <cfqueryparam value="#arguments.peIn#" cfsqltype="cf_sql_varchar">          and dateDeleted is NULL        </cfquery>                <cfreturn true>    </cffunction>      	<!---  ***	RETURN: Boolean for whether or not the student has completed the item 	***--->    <cffunction name="isProcessItemComplete" returntype="boolean">    	<cfargument name="bannerIDin">        <cfargument name="itemCodeIn">        <cfargument name="peIn" required="no" default="">                <cfquery name="checkItem" datasource="#application.roadtoDB#">        select ID        from processParent        where bannerid = <cfqueryparam value="#arguments.banneridin#" cfsqltype="cf_sql_varchar" maxlength="50">          and itemCode = <cfqueryparam value="#arguments.itemCodeIn#" cfsqltype="cf_sql_varchar" maxlength="50">          <cfif arguments.peIn NEQ ''>          and parentEmail = <cfqueryparam value="#arguments.peIn#" cfsqltype="cf_sql_varchar">          </cfif>          and date_deleted is null        </cfquery>		<cfreturn checkItem.recordCount GT 0>	</cffunction>        <!--- ***	RETURN: String for date	*** --->        <cffunction name="generalMonth" returntype="string">    	<cfargument name="dateIn">                <cfset dayP = datePart('d',arguments.dateIn)>                <cfif dayP LE 10>        	<cfset returnString = 'Early'>        <cfelseif dayP LE 20>        	<cfset returnString = 'Middle of'>        <cfelse>        	<cfset returnString = 'Late'>        </cfif>                <cfset returnString = returnString & ' ' & dateFormat(arguments.dateIn,'mmmm')>                <cfreturn returnString>	</cffunction>            <!--- ***	RETURN: Array of the next steps for the student	*** --->    <cffunction name="getNextSteps" returntype="array">        	<cfparam name="session.parentEmail" default="">                <!--- allow the date to be changed, for demo --->		<cfif isdefined("url.currentDate") and url.currentDate NEQ ''>            <cfset compDate = createDate(mid(url.currentDate,1,4),mid(url.currentDate,5,2),mid(url.currentDate,7,2))>        <cfelse>            <cfset compDate = now()>        </cfif>                <!--- *** Generate the Array *** --->        <cfset nextStepsArray = []>        <!--- Deposit --->                <!--- *** Process Item Code: deposit *** ---> 		<!--- This must be done for each step --->        <cfset stepStruct = {}>                <cfset stepStruct.itemCode = 'deposit'>                <cfif session.studentProcess.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>            <cfset stepStruct.status = 'Complete'>        <cfelse>            <cfset stepStruct.status = 'Active'>        </cfif>                <cfset stepStruct.type = 'auto'>        <cfset stepStruct.typeText = 'Submit Your Deposit to Complete'>                <cfset stepStruct.timeframe = ''>                <cfif session.studentType EQ 'T'>            <cfset stepStruct.title = 'Send In Your Deposit To Join the Xavier Family'>        <cfelseif session.international>            <cfset stepStruct.title = 'Submit Your Deposit'>        <cfelse>            <cfset stepStruct.title = 'Send In Your Deposit To Join The Class of 2019'>        </cfif>                <cfsavecontent variable="stepStruct.body">        <cfif session.gender EQ 'F'>        	<cfset studentHim = 'Her'>            <cfset studentHe = 'She'>            <cfset studentHis = 'Her'>        <cfelse>        	<cfset studentHim = 'Him'>            <cfset studentHe = 'He'>            <cfset studentHis = 'His'>        </cfif>        <cfoutput>        <p>Once your student has made #lcase(studentHis)# final decision that Xavier is the right place for #lcase(studentHim)#, all #lcase(studentHe)# needs to do to secure #lcase(studentHis)# spot in the class is submit #lcase(studentHis)# tuition and housing deposits. These are $200 each and can be <a href="/parent/about/deposit.cfm">submitted online</a> or by check. <a href="/parent/about/deposit.cfm">Visit the deposit page for more information.</a></p>        </cfoutput>        </cfsavecontent>                <cfset stepStruct.image = 'classof2013.jpg'>                <cfset arrayAppend(nextStepsArray,stepStruct)>        <!--- End Deposit Section ---><!--- FAFSA --->                <!--- *** Process Item Code: fafsa - Trad, Tran *** --->        <cfif isdefined("url.showAll") OR (NOT session.international) and datecompare(compDate, Application.fafsa_start) lt 0>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'fafsa'>                        <cfset stepStruct.status = 'Preview'>                        <cfset stepStruct.timeframe = '<span class="timeframe-label">Required Timeframe:</span> Begins #dateFormat(application.fafsa_start,'long')#, Complete by #dateFormat(application.fafsa_stop,'long')#'>                        <cfset stepStruct.title = 'File the Free Application for Federal Student Aid (FAFSA)'>                        <cfsavecontent variable="stepStruct.body">            <p>If you want to be considered for need-based financial assistance, complete the Free Application for Federal Student Aid (FAFSA) <a href="http://www.fafsa.ed.gov/" target="_blank">online</a> or request a paper copy of the FAFSA from 800-4FED-AID. The completed FAFSA should be submitted to the federal processor before Xavier's February 15 priority deadline; however, financial aid will still be reviewed for students who submit the FAFSA after the priority deadline. Be sure to designate Xavier to receive your results. Xavier's federal school code for the FAFSA is 003144.</p>            </cfsavecontent>                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>                <!--- *** Process Item Code: fafsa - Trad, Tran *** --->        <cfif isdefined("url.showAll") OR (NOT session.international) and datecompare(compDate, Application.fafsa_start) ge 0>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'fafsa'>                        <cfif session.studentProcess.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfif datecompare(compDate, Application.fafsa_stop) lt 0>                	<cfset stepStruct.status = 'Active'>                <cfelse>                	<cfset stepStruct.status = 'Passed'>                </cfif>            </cfif>                        <cfset stepStruct.type = 'auto'>            <cfset stepStruct.typeText = 'Will update when FAFSA is processed'>                        <cfset stepStruct.timeframe = '<span class="timeframe-label">Required Timeframe:</span> Complete by #dateFormat(application.fafsa_stop,'long')#'>                        <cfset stepStruct.title = 'File the Free Application for Federal Student Aid (FAFSA)'>                        <cfsavecontent variable="stepStruct.body">            <p>If you want to be considered for need-based financial assistance, complete the Free Application for Federal Student Aid (FAFSA) <a href="http://www.fafsa.ed.gov/" target="_blank">online</a> or request a paper copy of the FAFSA from 800-4FED-AID. The completed FAFSA should be submitted to the federal processor before Xavier's February 15 priority deadline; however, financial aid will still be reviewed for students who submit the FAFSA after the priority deadline. Be sure to designate Xavier to receive your results. Xavier's federal school code for the FAFSA is 003144.</p>            </cfsavecontent>                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end fafsa section ---><!--- Parent contact info --->		<!--- *** Process Item Code: parentContactInfo - Trad *** --->        <cfif isdefined("url.showAll") OR (datecompare(compDate, Application.parent_contact_start) lt 0)>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'parentContactInfo'>                        <cfset stepStruct.status = 'Preview'>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = '<b>Timeline:</b> Early May'>                        <cfset stepStruct.title = "Update Parent Contact Information">                        <cfsavecontent variable="stepStruct.body">             	<p>Once your deposit is submitted and we are ready, we will ask you to make sure your contact information is current. We'd like to ensure that all programming, communications, and announcements that you need are directed to you as quickly and efficiently as possible.</p>            </cfsavecontent>                        <cfset stepStruct.link = "/parent/parent-road/contact-info.cfm">                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>                <!--- *** Process Item Code: parentContactInfo - Trad *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.parent_contact_start) ge 0))>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'parentContactInfo'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode,session.parentEmail)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Update Parent Contact Information">                        <cfsavecontent variable="stepStruct.body">             	<p>Welcome to the Xavier Family!</p>                <p>We'd like to ensure that all programming, communications, and announcements that you need are directed to you as quickly and efficiently as possible.</p>                <p>Please take a few minutes to verify and augment all parent contact information that we have on file.</p>                 <p><b>Please note that when you update and save your contact information the new information gets transferred to a different database. If you need to review/update your contact information your old information will still be displayed. Your updated contact information is saved somewhere else.</b></p>                                              <!--- see if we have any data in the sheet --->                <!--- load the data from Excel --->                <cftry>                <cfspreadsheet action="read" src="//nocwebvfs/webcontent/roadto/live/parent/parent-road/2015-data.xlsx" sheet="1" query="parents" >                <cfcatch>                </cfcatch>                </cftry>                                <cftry>                <cfquery name="getParent" dbtype="query">                select *                from parents                where col_2 = <cfqueryparam value="#session.bannerid#" cfsqltype="cf_sql_varchar">                </cfquery>                                <!--- see if we have a valid P1 --->                <cfif getParent.recordcount GT 0 and getParent.col_10 NEQ ''>                	<cfset parentName = "#getParent.col_7# #getParent.col_8#">                                        <!--- check to see if this parent has already been submitted --->                    <cfquery name="checkP1" datasource="FB-2">                    select ID from form1000                     where value=<cfqueryparam value="P1" cfsqltype="cf_sql_varchar">                      and submissionID in (	select submissionID from form1000                       						where value=<cfqueryparam value="#session.bannerid#" cfsqltype="cf_sql_varchar">)                    </cfquery>                                        <cfoutput>                    <p>                    <a href="/parent/parent-road/contact-info.cfm?parentEmail=#getParent.col_10#" class="toPage">                  	<cfif checkP1.recordcount GT 0>Resubmit<cfelse>Submit</cfif>                     Contact Info For 					<cfif trim(parentName) NEQ ''>#parentName# (#getParent.col_10#)<cfelse>#getparent.col_10#</cfif>                    </a>                    </p>                    </cfoutput>                </cfif>                                 <!--- see if we have a valid P2 --->                <cfif getParent.recordcount GT 0 and getParent.col_29 NEQ ''>                	<cfset parentName = "#getParent.col_26# #getParent.col_27#">                                        <!--- check to see if this parent has already been submitted --->                    <cfquery name="checkP2" datasource="FB-2">                    select ID from form1000                     where value=<cfqueryparam value="P2" cfsqltype="cf_sql_varchar">                      and submissionID in (	select submissionID from form1000                       						where value=<cfqueryparam value="#session.bannerid#" cfsqltype="cf_sql_varchar">)                    </cfquery>                                        <cfoutput>                    <p>                    <a href="/parent/parent-road/contact-info.cfm?parentEmail=#getParent.col_29#" class="toPage">                  	<cfif checkP2.recordcount GT 0>Resubmit<cfelse>Submit</cfif>                     Contact Info For 					<cfif trim(parentName) NEQ ''>#parentName# (#getParent.col_29#)<cfelse>#getparent.col_29#</cfif>                    </a>                    </p>                    </cfoutput>                </cfif>                                   <cfcatch>                </cfcatch>                </cftry>                         	                <!--- generic info --->                <p><a href="/parent/parent-road/contact-info.cfm" class="toPage">Submit Contact Info For Another Parent</a></p>            </cfsavecontent>                        <cfset stepStruct.link = "/parent/parent-road/contact-info.cfm">                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>                <!--- end parent contact update --->                 <!--- *** Process Item Code: parentContactInfo - Trad *** --->        <cfif isdefined("url.showAll") OR (datecompare(compDate, Application.parent_learn_more_stop) lt 0)>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'parentLearnMore'>                        <cfset stepStruct.status = 'Preview'>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = '<b>Timeline:</b> Early May'>                        <cfset stepStruct.title = "Learn More About Xavier">                        <cfsavecontent variable="stepStruct.body">             	<p>Throughout the spring and summer new items will be added to your next steps for review. This will include information pages about Xavier and the services offered at Xavier. It will also include actions you or your child need to take before classes start in August. Stay tuned for more details.</p>            </cfsavecontent>                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>                		<!--- Campus Housing Message --->		<!--- *** Process Item Code: parentHousingMessage - Trad *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.housing_contract_start) ge 0) and session.housingDeposit EQ '1')>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'parentHousingMessage'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Review the Housing Agreement">                        <cfsavecontent variable="stepStruct.body">            <p> Dear Parent, </p>            <p> Thank you for being involved as your student prepares to experience Xavier. We are glad that you and your student have decided to join the Xavier family!              About 90% of the incoming first year class will transition to their Xavier experience by living on-campus. As you may have likely at some point during the              Admission process, Xavier has a two year on-campus residency requirement for students living further than 35-miles from the campus. This requirement is for <strong><u>all</u></strong> students new to the University (incoming first year students, transfer students, students who are international, etc.) </p>            <p> Xavier requires this for several reasons - national and Xavier specific institutional research shows students who live on campus: </p>            <p> 1) have higher grade point averages, <br>              2) are more likely to stay at Xavier, <br>              3) are more likely to be involved on-campus, and <br>              4) are more likely to graduate within 4 years. </p>            <p> Due to these factors, the University has made our on-campus housing facilities a priority and believes that the first two years provide a solid foundation              for students to be successful academically and socially. </p>            <p> During the process of applying for housing your student will be asked to read and electronically indicate (considered their electronic signature) that they              have read and agree to all provisions of the Housing Agreement. <strong>Item number 3 in the Agreement is agreeing to live on-campus for their first <u>two</u> years at Xavier.</strong> Some of your students may choose              to read and share this information with you, while others may not. You can view a copy of the agreement <a href="/housing/first-year-housing-agreement.pdf" target="_blank">here</a>.</p>            <p> As young adults who are 18 years of age, they are legally permitted to sign such an agreement and be held responsible for fulfilling the obligations of the              agreement. If your student is younger than 18 years of age, they will be asked to print a copy of the agreement, sign it, obtain your signature and return              a paper copy to the University. The electronic signature and the signature on the paper copy are both legally binding documents and thus I want to be sure              you are aware of the terms of the agreement. </p>            <p> Very soon students will be able to indicate their housing preferences and even select their own roommate through an on-line process. As a parent/guardian,              any guidance you can give them about being honest in their responses and making sure they are picking their roommate based upon more than one              characteristic would be helpful to them making a good roommate match for their first year at Xavier. The staff in the Office of Residence Life understands              and appreciates how important the housing and roommate process is for the student experience. We will do everything we can to assist students through the              process prior to arrival and after their arrival. We look forward to welcoming your student to campus in August! </p>            <p> Sincerely, </p>            <p> Lori <br>              Lori Lambert <br>              Senior Director for Student Affairs <br>              <a href="mailto:Lambert@xavier.edu">Lambert@xavier.edu</a> </p>            </cfsavecontent>                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end Parent Housing Message --->           		<!--- Jesuit Identity Static Page --->		<!--- *** Process Item Code: jesuitIdentityStaticPage - Trad *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.jesuit_process_start) ge 0))>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'jesuitIdentityStaticPage'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Learn More About Xavier's Jesuit Identity">                        <cfsavecontent variable="stepStruct.body">             <p>Book smarts will only get you so far. At Xavier, you'll develop knowledge for sure. But you'll also develop values, spiritual growth, responsibility for others and a love for learning - the stuff that really prepares you to function as an active member of the global community.</p>                          <p>Watch the faith and justice video to learn more about Xavier's Jesuit Identity.</p>                        <p><a href="/parent/about/jesuit-identity.cfm" class="toPage">View the Jesuit Identity Page Now</a></p>            </cfsavecontent>                        <cfset stepStruct.link = "/parent/about/jesuit-identity.cfm">                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end Jesuit Identity Page --->                                <!--- Financial Literacy Static Page --->		<!--- *** Process Item Code: financialLiteracyPage - Trad *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.financial_literacy_start) ge 0))>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'financialLiteracyPage'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Learn More About Financial Literacy">                        <cfsavecontent variable="stepStruct.body">                          <p>Watch the videos and review the content provided by the Bursar's Office and the Office of Student Financial Assistance to learn more about paying for your Xavier education.</p>                        <p><a href="/parent/parent-road/financial-literacy.cfm" class="toPage">View the Financial Literacy Page Now</a></p>            </cfsavecontent>                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end Financial Literacy Page --->         <!--- Wellness Static Page --->		<!--- *** Process Item Code: wellnessParentPage - Trad *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.wellness_general_start) ge 0))>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'wellnessParentPage'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Learn More About Wellness Services">                        <cfsavecontent variable="stepStruct.body">                         <p>The goal of Wellness Services is to provide you with resources that will help you to enjoy a healthy and safe educational experience.</p>                        <p>Review the Wellness Services page to learn about what services they offer.</p>                        <p><a href="/parent/parent-road/wellness-general.cfm" class="toPage">View the Wellness Services Page Now</a></p>            </cfsavecontent>                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end Wellness Page --->             <!--- Auxiliary Services Page --->		<!--- *** Process Item Code: auxServParentPage - Trad *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.auxServParentPage_start) ge 0))>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'auxServParentPage'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Learn More About Auxiliary Services">                        <cfsavecontent variable="stepStruct.body">                         <p>The Office of Auxiliary Services provides support for many student based business services. As parents of our incoming students we want to keep you well informed.  Review a brief description of the services we provide, as well as how we are communicating this information to your students within the Road To Xavier portal and at various on-campus events.</p>                        <p><a href="/parent/parent-road/auxiliary-services.cfm" class="toPage">View the Auxiliary Services Page Now</a></p>            </cfsavecontent>                                  <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end Auxiliary Services Page --->                <!--- Wellness - Health Insurance Waiver --->		<!--- *** Process Item Code: wellnessWaiver - ALL *** --->        <cfif isdefined("url.showAll") OR (session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.wellness_waiver_start) ge 0)>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'wellnessWaiver'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = 'Student Health Insurance Waiver'>                        <cfsavecontent variable="stepStruct.body">                <p>All students are required to have health insurance coverage. Students are automatically charged for and enrolled in Xavier's student health insurance plan. The health insurance fee may be waived by providing proof of comparable health insurance coverage.</p>                                <ol class="inItem">                    <li>Go to <a href="http://www.gallagherstudent.com/xavier">http://www.gallagherstudent.com/xavier</a></li>                    <li>Click on Enroll/Waive and follow the instructions</li>                    <li>The waiver period opens <strong>July 1, 2015</strong>.</li>                    <li>The waiver period closes on<strong> September 15, 2015</strong>.</li>                    <li>Waiver requests will not be accepted after <strong>September 30, 2015</strong>.</li>                </ol>            </cfsavecontent>                        <cfset stepStruct.link = "http://www.gallagherstudent.com/xavier">            <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end Wellness Health Insurance Waiver section --->                         <!---Communications Survey --->		<!--- *** Process Item Code: communicationsSurvey - ALL *** --->        		<!---  <cfif isdefined("url.showAll") OR (this.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.comm_survey_start) ge 0)>        			<!--- This must be done for each step --->                        <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'communicationsSurvey'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = 'Complete The Communications Survey'>                        <cfsavecontent variable="stepStruct.body">                <p>Now that you can look back on the path to making the big college decision, we'd love to hear from you about your experience with communications from Xavier along the way. What did you like? What did we miss? What can we improve on for the Class of 2020?</p>				<p><a href="/parent/parent-road/communicationSurvey.cfm" class="button radius">Take The Survey Now</a></p>            </cfsavecontent>                        <cfset stepStruct.link = "/parent/parent-road/communicationSurvey.cfm">                        <cfset stepStruct.topic = 'ToDo'>                        <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        --->                        <!--- end Communications Survey --->                                                       <!--- Retention & Parent Outreach Info --->		<!--- *** Process Item Code: retentionInfo - ALL *** --->        <cfif isdefined("url.showAll") OR (session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.success_advisor_start) ge 0)>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'retentionInfo'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = 'Student Success and Parent/Family Outreach'>                        <cfswitch expression="#left(session.lastname,1)#">                <cfcase value="A,B,C,D,E,F,G,H,I">                    <cfset saName = 'Laura Frazier'>                    <cfset saUsername = 'frazier'>                </cfcase>                                <cfcase value="J,K,L,M,N,O,P,Q,R">                    <cfset saName = 'Molly McDaniel'>                    <cfset saUsername = 'maher'>                </cfcase>                                <cfcase value="S,T,U,V,W,X,Y,Z">                    <cfset saName = 'Luther Smith'>                    <cfset saUsername = 'smith'>                </cfcase>                                <cfdefaultcase>                               <cfmail from="lieslandr@xavier.edu" to="lieslandr@xavier.edu" subject="R2X Student Success Coach Missing" type="html">                        <cfdump var="#session#" label="session">                        <cfdump var="#cgi#" label="CGI">                    </cfmail>                                    <cfset saName = 'Luther Smith'>                    <cfset saUsername = 'smith'>                </cfdefaultcase>                        </cfswitch>                        <cfsavecontent variable="stepStruct.body">                <cfoutput>                <p>As your child looks forward to the beginning of his or her Xavier career, know that the Office of Student Success and Parent/Family Outreach is here to serve you and your child. The start of college brings new and exciting experiences, as well as some uncertainties and challenges. This is where our office is here to act as your advocate and support system.</p>                <p> As #trim(session.firstName)#'s Student Success Coach, I am here to act as your first point of contact when you find you are in need of support. Whether it is academic or social, whether #trim(session.firstName)# is falling behind in a class, or you need help with creating a financial plan that works for you. I am here for you. </p>                <p> If I can help in any way, please reach out to our office at 513-745-3036. </p>                            <p>Sincerely,<br />                #saName#</p>				</cfoutput>            </cfsavecontent>                        <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end Retention and Parent Info section --->                 <!--- FERPA --->		<!--- *** Process Item Code: bursarFERPA - ALL *** --->        <cfif isdefined("url.showAll") OR (session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.ferpa_form_start) ge 0)>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'bursarFERPA'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = 'FERPA Release'>                        <cfsavecontent variable="stepStruct.body">                <p>The Family Education Rights and Privacy Act (FERPA) is a Federal law that protects the privacy of student education, financial and academic records. For the student's protection, FERPA limits release of student record information without the student's explicit written consent; however it also gives the student's parent(s)/guardian(s) the right to review those records if the parent(s)/guardian(s) claim the student as a dependent on their Federal Income Tax Return.</p>                <p><a href="/parent/parent-road/ferpa.cfm" class="toPage">Learn More and Complete the FERPA Release</a></p>            </cfsavecontent>                        <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end FERPA section --->                                                                                  <!--- Student Handbook --->		<!--- *** Process Item Code: handbookAgreement - ALL *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.handbook_start) ge 0))>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'handbookAgreement'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Student Handbook">                        <cfsavecontent variable="stepStruct.body">                          <p>The Xavier University Student Handbook is now available for your review. Your student, through the Road to Xavier, is asked to review the handbook and submit the handbook agreement indicating that they have reviewed and agree to the handbook. You can review the handbook at <a href="http://www.xavier.edu/handbook/" target="_blank">http://www.xavier.edu/handbook/</a>.</p>                        <p><a href="http://www.xavier.edu/handbook/" class="toPage" target="_blank">View the Student Handbook Now</a></p>            </cfsavecontent>                         <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end handbook --->                <!--- Safety --->		<!--- *** Process Item Code: safety - ALL *** --->        <cfif isdefined("url.showAll") OR ((session.studentProcess.isProcessItemComplete(session.bannerid,'deposit') and datecompare(compDate, Application.safety_start) ge 0))>			<!--- This must be done for each step --->            <cfset stepStruct = {}>                        <cfset stepStruct.itemCode = 'safetyInfo'>                        <cfif this.isProcessItemComplete(session.bannerid,stepStruct.itemCode)>                <cfset stepStruct.status = 'Complete'>            <cfelse>            	<cfset stepStruct.status = 'Active'>            </cfif>                        <cfset stepStruct.type = 'manual'>            <cfset stepStruct.typeText = ''>                        <cfset stepStruct.timeframe = ''>                        <cfset stepStruct.title = "Campus Safety">                        <cfsavecontent variable="stepStruct.body">                         <p>Families and students are always concerned about safety and want to know &ldquo;How safe is the Xavier campus?&rdquo; simply and directly, <strong>Very Safe</strong>.  Campus crimes decreased by 27% during the 2009-2010 academic year as  compared to the previous year, even though the residential population on  campus grew by 17%. In fact, personal property crime has declined 30%  over the last four years. However, it is important to note Xavier is  part of a larger urban community and we make the personal safety and  security of all students, faculty, staff and visitors a top priority.</p>			<p>The University provides a voice, text message, and email system, XU ALERT ME. This communication system contacts members of the campus community through     voice, text messages and emails in the event of an urgent situation. Students may sign up for the service through the MyXU campus portal.</p>			<p>The way in which the University contacts members of the campus community about safety issues differs depending on the nature of the incident.</p>                        <p><a href="/parent/parent-road/safety.cfm" class="toPage">Learn More About Campus Safety</a></p>            </cfsavecontent>                         <cfset arrayAppend(nextStepsArray,stepStruct)>        </cfif>        <!--- end safety --->    	                <cfreturn nextStepsArray>        </cffunction>            <!--- *** Parental Access Section*** ---><cffunction name="checkAccountAccess" returntype="boolean">	<cfargument name="idIn">    <cfargument name="emailIn">        <!--- get the parents --->    <cfquery name="checkAccess" datasource="#application.roadtoDB#">    select ID    from processParentAccess    where bannerid  = <cfqueryparam value="#arguments.idIn#" cfsqltype="cf_sql_varchar">      and parent_email = <cfqueryparam value="#arguments.emailIn#" cfsqltype="cf_sql_varchar">      and date_deleted is null   	</cfquery>        <cfreturn checkAccess.recordCount GT 0></cffunction><cffunction name="verifyProcessPageAccess" returntype="void">    <cfif not checkAccountAccess(session.bannerid,session.parentEmail)>    	<cflocation url="https://roadto.xavier.edu/parent/parent-road/access-denied.cfm" addtoken="no">    </cfif></cffunction>    </cfcomponent>