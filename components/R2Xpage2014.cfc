<cfcomponent>	<cffunction name="loadPageStart" access="public" returntype="string">    	<!--- optional preset request variables include:			titleAddition			customHeadCode		--->		        <cfoutput>        <cfsavecontent variable="returnHTML">        	<!doctype html>            <html class="no-js" lang="en">            <head>                <meta charset="utf-8" />                <meta name="viewport" content="width=device-width,initial-scale=1.0" />                <title>Road to Xavier<cfif isdefined("request.titleAddition")> - #request.titleAddition#</cfif></title>                <link rel="stylesheet" href="/styles/foundation.css" />                <link rel="stylesheet" href="/styles/global.css" />                                <script src="/scripts/vendor/modernizr.js"></script>                <script src="/scripts/vendor/jquery.js"></script> 				<!---icon font stylesheets--->                <link href="/styles/r2x-icon-font.css" rel="stylesheet">                <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">                                <cfif isdefined("request.customHeadCode")>                	#request.customHeadCode#                </cfif>                                <!--- analytics. This should remain last --->				<cfif isdefined('session.bannerid') and trim(session.bannerid) EQ '000300001'>                	<!-- analytics excluded for Kristyn -->                <cfelse>                	<cfinclude template="#Application.AppBasePath#/library/framework/element-ga.cfm">				                </cfif>                        </head>            <body>                  <!--- GTM template include --->                <cfinclude template="#Application.AppBasePath#/library/framework/element-gtm.cfm">                                <!--- Header and navigation template include --->                                <cfinclude template="#Application.AppBasePath#/library/framework/element-header.cfm">                                                                <div class="container main">                </cfsavecontent>        		<cfreturn returnHTML>                </cfoutput>	</cffunction>        <cffunction name="loadPageEnd" access="public" returntype="string">    	<!--- optional preset request variables include:			customFooterCode		--->		        <cfoutput>        <cfsavecontent variable="returnHTML">        		</div>	                <cfinclude template="#Application.AppBasePath#/library/framework/element-footer.cfm">                                <script src="/scripts/foundation.min.js"></script>                <script>                $(document).foundation();                </script>                                <cfif isdefined("request.customFooterCode")>                	#request.customFooterCode#                </cfif>                            </body>            </html>                </cfsavecontent>        		<cfreturn returnHTML>                </cfoutput>	</cffunction></cfcomponent>